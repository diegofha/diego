void main()
{
  int	i,n,id,n1,p1,p2,p3,nf,ef;
  
cout<< "escribir cantidad de alumnos: ";
cin>> n;
for (i=1;i<=n;i++){

cout<<" escribir id del estudiante: ";
cin>> id;
cout<<"escribir nota practica";
cin>> n1;
cout<<"escribir parcial 1: ";
cin>> p1;
cout<<"escribir parcial 2: ";
cin>> p2;
cout<<"escribir parcial 3: ";
cin>> p3;
cout<<"escribir nota del examen final: ";
cin>> ef;
	nf= n1*0.2+((p1+p2+p3)/3)*0.4+ef*0.4;

cout<<"la nota final es:"<<nf<<endl;
cin>>nf;}
getch ();
}
	

